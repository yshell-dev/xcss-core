{
  "name": "xcss-core",
  "description": "A powerful CSS kernel for building customized, scalable, and high-performance frontend frameworks. Seamlessly blend the raw potential of CSS with the ease of pre-made utilities, managed by a core orchestrator that optimizes HTML and JavaScript performance for modern web applications.",
  "version": "0.0.9",
  "main": "package.js",
  "exports": "./package.js",
  "typings": "package.d.ts",
  "type": "module",
  "bin": {
    "xcss": "./package.js",
    "xdev": "./source/scripts/live.sh"
  },
  "scripts": {
    "install": "chmod +x ./package.js && ./package.js",
    "profile": "go install github.com/moderato-app/live-pprof@latest && live-pprof",
    "lint": "cd source && golangci-lint run && cd ..",
    "build": "cd source && chmod +x ./scripts/build.sh && ./scripts/build.sh && cd ..",
    "stats": "chmod +x ./source/scripts/stats.sh && ./source/scripts/stats.sh source",
    "package": "node package.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yshell-dev/xcss-package"
  },
  "keywords": [
    "css",
    "xcss",
    "styles",
    "semantic",
    "frontend",
    "design",
    "framework"
  ],
  "author": {
    "name": "Vyshnav Prasad",
    "url": "https://www.yshell.dev"
  },
  "license": "MIT",
  "homepage": "https://www.xcss.io",
  "bugs": {
    "url": "https://github.com/yshell-dev/xcss-package/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/yshell-dev"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}